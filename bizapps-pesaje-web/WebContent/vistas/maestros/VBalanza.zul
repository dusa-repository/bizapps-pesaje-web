<zk>
	<style src="../public/Estilo.css" />
	<div id="divBalanza" apply="controlador.maestros.CBalanza">
		<groupbox width="auto" mold="3d" title="Balanza (VBalanza)">
			<div id="botoneraBalanza" />
			<groupbox id="gpxRegistro" width="auto" class="azul" title=""
				open="false">
				<caption label="AÃ±adir" style="font-weight:bold; font-size: 13px"
					tooltiptext="Presione para Registrar una nueva Balanza" />
				<separator />
				<grid width="100%">
					<columns visible="false">
						<column label="" width="20%" />
						<column label="" width="10%" />
						<column label="" width="25%" />
						<column label="" width="45%" />
					</columns>
					<rows>

						<row>
							<hbox>
								<label class="etiqueta" value="Descripcion:" />
								<space />
								<label value="*" style="font-weight:bold;color:red">
								</label>
							</hbox>
							<cell colspan="2">
								<textbox id="txtDescripcion" hflex="1" maxlength="100"
									tooltiptext="Ingrese la Descripcion" width="100%" />
							</cell>
							<hbox />
						</row>
						<row>
							<label class="etiqueta" value="Ip:" />
							<cell colspan="2">
								<textbox id="txtIp" hflex="1" maxlength="500"
									tooltiptext="Ingrese la Ip" width="100%" />
							</cell>
							<hbox />
						</row>
						<row>
							<label class="etiqueta" value="Puerto:" />
							<cell colspan="2">
								<textbox id="txtPuerto" hflex="1" maxlength="500"
									tooltiptext="Ingrese el Puerto" width="100%" />
							</cell>
							<hbox />
						</row>
						<row>
							<label class="etiqueta" value="# Equipo:" />
							<cell colspan="2">
								<textbox id="txtEquipo" hflex="1" maxlength="500"
									tooltiptext="Ingrese el Equipo" width="100%" />
							</cell>
							<hbox />
						</row>
						<row>
							<label class="etiqueta" value="Modelo:" />
							<cell colspan="2">
								<textbox id="txtModelo" hflex="1" maxlength="500"
									tooltiptext="Ingrese el Modelo" width="100%" />
							</cell>
							<hbox />
						</row>
						<row>
							<label class="etiqueta" value="Baud Rate:" />
							<cell colspan="2">
								<textbox id="txtBaudRate" hflex="1" maxlength="500"
									tooltiptext="Ingrese el Baud Rate" width="100%" />
							</cell>
							<hbox />
						</row>
						<row>
							<label class="etiqueta" value="Data Bits:" />
							<cell colspan="2">
								<textbox id="txtDataBits" hflex="1" maxlength="500"
									tooltiptext="Ingrese el Data Bits" width="100%" />
							</cell>
							<hbox />
						</row>
						<row>
							<label class="etiqueta" value="Stop Bits:" />
							<cell colspan="2">
								<textbox id="txtStopBits" hflex="1" maxlength="500"
									tooltiptext="Ingrese el Stop Bits" width="100%" />
							</cell>
							<hbox />
						</row>
						<row>
							<label class="etiqueta" value="Parity None:" />
							<cell colspan="2">
								<textbox id="txtParityNone" hflex="1" maxlength="500"
									tooltiptext="Ingrese el Parity None" width="100%" />
							</cell>
							<hbox />
						</row>
						<row>
							<label class="etiqueta" value="Clasificacion:" />
							<cell colspan="2">
								<radiogroup id="rdbClasificacion">
									<radio id="rdoIP" label="IP" style="font-weight:bold" />
									<separator />
									<radio id="rdoRS" label="RS-232" style="font-weight:bold" />
								</radiogroup>
							</cell>
							<hbox />
						</row>
					</rows>
				</grid>
			</groupbox>
			<separator />
			<groupbox id="gpxDatos" class="azul" width="auto">
				<caption label="Registros" style="font-weight:bold; font-size: 13px;"
					tooltiptext="Presione para Visualizar las Balanzas Registradas" />
				<div id="divCatalogoBalanza" tooltiptext="Seleccione la Balanza para Realizar una Accion" />
			</groupbox>
			<separator />
			<separator />
		</groupbox>
	</div>
</zk>